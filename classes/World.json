		"prefix": "World:SetPPBloom"
	},
	"World:SetPPChromaticAberration": {
		"body": "World:SetPPChromaticAberration(${1:intensity}, ${2:start_offset})",
		"description": "Set Post Process Chromatic Aberration Settings",
		"prefix": "World:SetPPChromaticAberration"
	},
	"World:SetPPImageEffects": {
		"body": "World:SetPPImageEffects(${1:vignette_intensity}, ${2:grain_jitter}, ${3:grain_intensity})",
		"description": "Set Post Process Image Effect Settings",
		"prefix": "World:SetPPImageEffects"
	},
